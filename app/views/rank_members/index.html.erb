<p id="notice"><%= notice %></p>

<h1>Listing Rank Members</h1>

<table>
  <thead>
    <tr>
      <th>Rank</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rank_members.each do |rank_member| %>
      <tr>
        <td><%= rank_member.rank_id %></td>
        <td><%= rank_member.student_id %></td>
        <td><%= link_to 'Show', rank_member %></td>
        <td><%= link_to 'Edit', edit_rank_member_path(rank_member) %></td>
        <td><%= link_to 'Destroy', rank_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rank member', new_rank_member_path %>
