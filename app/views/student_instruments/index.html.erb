<h1>Listing student_instruments</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Instrument</th>
      <th>Ensemble</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_instruments.each do |student_instrument| %>
      <tr>
        <td><%= student_instrument.student_id %></td>
        <td><%= student_instrument.instrument_id %></td>
        <td><%= student_instrument.ensemble_id %></td>
        <td><%= link_to 'Show', student_instrument %></td>
        <td><%= link_to 'Edit', edit_student_instrument_path(student_instrument) %></td>
        <td><%= link_to 'Destroy', student_instrument, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student instrument', new_student_instrument_path %>
