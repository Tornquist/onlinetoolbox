<h1>Listing claimed_students</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @claimed_students.each do |claimed_student| %>
      <tr>
        <td><%= claimed_student.student_id %></td>
        <td><%= claimed_student.user_id %></td>
        <td><%= link_to 'Show', claimed_student %></td>
        <td><%= link_to 'Edit', edit_claimed_student_path(claimed_student) %></td>
        <td><%= link_to 'Destroy', claimed_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Claimed student', new_claimed_student_path %>
