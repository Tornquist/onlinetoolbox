<p id="notice"><%= notice %></p>

<h1>Listing Section Members</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @section_members.each do |section_member| %>
      <tr>
        <td><%= section_member.student_id %></td>
        <td><%= section_member.section_id %></td>
        <td><%= link_to 'Show', section_member %></td>
        <td><%= link_to 'Edit', edit_section_member_path(section_member) %></td>
        <td><%= link_to 'Destroy', section_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section member', new_section_member_path %>
